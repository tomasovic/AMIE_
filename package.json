  "main": "src/launcher.ts",
  "scripts": {
    "generate": "node ./dist/src/utils/types/generate-schema.js && graphql-codegen --config codegen.yaml",
    "compile": "tsc && yarn run generate && node --loader ts-node/esm ./dist/src/launcher.js",
    "start": "nodemon -e ts --exec \"yarn run compile\"",
    "seed": "ts-node --transpile-only prisma/seed.ts",
    "reset": "npx prisma migrate reset",
    "test:unit": "jest --forceExit __tests__/unit/**",
    "test:int": "jest --forceExit __tests__/integration/**",
    "test:e2e": "jest --forceExit --runInBand __tests__/e2e/**",
    "test:watch": "jest --watch",
    "test": "jest"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-resolvers": "^4.0.1",
    "@types/bcryptjs": "^2.4.3",
    "@types/fs-extra": "^11.0.1",
    "@types/glob": "^8.1.0",
    "@types/node-cron": "^3.0.8",
    "@types/passport": "^1.0.12",
    "@types/passport-jwt": "^3.0.9",
    "@types/passport-twitter": "^1.0.37"
  },
