    "@types/jest": "^29.5.5",
    "@types/nodemailer": "^6.4.10",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.11.1",
    "apollo-server-express": "^3.12.1",
    "axios": "^1.5.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "class-validator": "^0.14.0",
